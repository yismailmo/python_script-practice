# A script of a dictionery to map characters to smiley faces using function

def emoji_converter(message):
    
    words = message.split(" ")
    emojis = {
    ":)" : "ðŸ˜€",
    ";)" : "ðŸ˜€"
 }
    output = ""
    for word in words:
        output += emojis.get(word, word) + " "
    return output
    
    
message = input(">")
print(emoji_converter(message))
    
